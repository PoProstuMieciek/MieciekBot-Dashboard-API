{
    "name": "mieciekbot-dashboard-api",
    "version": "4.0.0",
    "main": "src/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "build:watch": "tsc -w",
        "start": "node dist/index.js",
        "start:dev:js": "node --require dotenv/config --inspect=9230 dist/src/index.ts",
        "start:dev:ts": "node --require ts-node/register --require dotenv/config --inspect=9230 src/index.ts",
        "dev:js": "nodemon -e js --exec npm run start:dev:js",
        "dev:ts": "nodemon -e ts --exec npm run start:dev:ts",
        "clean": "rm -rf dist/"
    },
    "dependencies": {
        "connect-mongo": "^4.4.1",
        "cors": "^2.8.5",
        "crypto-js": "^4.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "mongoose": "^5.12.10",
        "node-fetch": "^2.6.1",
        "passport": "^0.4.1",
        "passport-discord": "^0.1.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.10",
        "@types/crypto-js": "^4.0.1",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.3",
        "@types/node": "^15.3.0",
        "@types/node-fetch": "^2.5.10",
        "@types/passport": "^1.0.6",
        "@types/passport-discord": "^0.1.3",
        "dotenv": "^9.0.2",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "author": "Maciej Opali≈Ñski <poprostumieciek@gmail.com>",
